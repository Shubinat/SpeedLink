<Page x:Class="SpeedLinkApplication.Pages.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SpeedLinkApplication.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Главная">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Список пользователей" Name="BtnUsers"  Width="200" Click="BtnUsers_Click"/>
            <Button Content="Список оборудования" Name="BtnEquipments" Click="BtnEquipments_Click"/>
            <Button Content="Список услуг" Name="BtnServices" Click="BtnServices_Click" />
            <Button Content="Список заявок" Name="BtnOrders" Click="BtnOrders_Click"/>
        </StackPanel>
    </Grid>
</Page>
